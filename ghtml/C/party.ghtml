            ''"<table class=gnotime-project-status bgcolor=#e0e0e0 width=500 border=0 cellpadding=4><tr><td>\n"
            ''"<b>"       gtt-project-title   ''"</b>"
            ''" -- "      gtt-project-desc    ''"\n"
            ''"<table class=gnotime-project-status--notes bgcolor=#f8f8f8 width=490 border=0 cellpadding=4><tr><td>\n"
                          gtt-project-notes   
            ''"</td></tr></table>\n\n"
            ''"</td></tr></table>\n\n"
            ))
?>

